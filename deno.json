{
  "name": "@cavity/backend",
  "version": "2.0.0",
  "exports": "./api/main.ts",
  "tasks": {
    "dev": "deno run --allow-net --allow-read --allow-env --allow-sys --watch ./api/main.ts",
    "start": "deno run --allow-net --allow-read --allow-env --allow-sys ./api/main.ts",
    "test": "deno test --allow-env",
    "lint": "deno lint",
    "fmt": "deno fmt"
  },
  "imports": {
    "@oak/oak": "jsr:@oak/oak@17.2.0",
    "@oak/oak/testing": "jsr:@oak/oak@17.2.0/testing",
    "@std/assert": "jsr:@std/assert@1.0.17",
    "@std/testing": "jsr:@std/testing@1.0.17",
    "@std/log": "jsr:@std/log@^0.224.14",
    "@std/io": "jsr:@std/io@^0.225.3",
    "@std/fmt": "jsr:@std/fmt@1.0.5",
    "postgres": "jsr:@db/postgres@^0.19.5",
    "supabase": "jsr:@supabase/supabase-js@2.44.3",
    "djwt": "https://deno.land/x/djwt@v3.0.2/mod.ts",
    "bcrypt": "https://deno.land/x/bcrypt@v0.4.1/mod.ts",
    "case": "https://deno.land/x/case@2.2.0/mod.ts",
    "sentry": "https://deno.land/x/sentry@7.107.0/index.mjs"
  },
  "lint": {
    "rules": {
      "tags": ["recommended"]
    }
  },
  "fmt": {
    "useTabs": false,
    "lineWidth": 100,
    "indentWidth": 2,
    "semiColons": true,
    "singleQuote": false
  },
  "lock": {
    "frozen": false
  },
  "nodeModulesDir": "manual"
}
